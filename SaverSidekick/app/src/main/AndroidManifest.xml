<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.saversidekick">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/saversidekick"
        android:label="Saver SideKick"
        android:supportsRtl="true"
        android:theme="@style/Theme.SaverSidekick"
        tools:targetApi="31">
        <activity
            android:name=".UpcomingBudgeActivity"
            android:label="My Upcoming Expense"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EarningsActivity"
            android:exported="true"
            android:label="@string/earnings_activity_label" />
        <activity
            android:name=".GoalsActivity"
            android:exported="true"
            android:label="Goals" />
        <activity
            android:name=".HomePageActivity"
            android:exported="true"
            android:label="Home Page"
            android:screenOrientation="unspecified" />
    </application>

</manifest>